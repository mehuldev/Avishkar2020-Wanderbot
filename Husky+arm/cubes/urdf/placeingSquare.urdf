<?xml version="0.0" ?>
<robot name="cube.urdf">
  <link name="baseLink">
    <contact>
    	<friction_anchor/>
    	   <lateral_friction value="3.0"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value=".1"/>
       <inertia ixx="0.001" ixy="0" ixz="0" iyy="1" iyz="0" izz="0.001"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
				<mesh filename="cubes/meshes/cube.obj" scale=".2 .2 .0005"/>
      </geometry>
       <material name="white">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	 	<box size=".2 .2 .0005"/>
      </geometry>
    </collision>
  </link>


#########################

  <link name="link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
  </link>

  <joint name="link-base" type="fixed">
    <parent link="baseLink"/>
    <child link="link"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>
########################

</robot>

